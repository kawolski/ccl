{
  "name": "ccl",
  "version": "1.0.0",
  "description": "Code Coverage Testing",
  "main": "app.js",
  "scripts": {
    "test": "jest --ci=true --config Jest.config.js",
    "buildaa": "nyc --reporter=cobertura --reporter=html ./node_modules/.bin/mocha tests/**/*.js --reporter mocha-junit-reporter --reporter-options mochaFile=./TEST-RESULTS.xml",
    "jestaa": "jest --coverage --env=node --runInBand",
    "testaa": "nyc jest --env=node --runInBand",
    "testAllaa": "nyc --all jest --env=node",
    "reportaa": "nyc report --reporter=html --report-dir=nyc_coverage"
  },
  "author": "Prashant",
  "license": "ISC",
  "dependencies": {
    "ci": "^2.2.0",
    "express": "^4.18.2",
    "jest": "^29.2.2",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "jest-junit": "^14.0.1"
  }
}
